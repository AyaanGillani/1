  var keywords=["marshmallow","screwdriver","volcano","trapezoid","saxophone","pancake","astronaut","jellyfish","blender","hammock"];
  var urls=["https://www.spacelint.org","https://www.purplekoala.biz","https://www.butterpickle.net","https://www.fishumbrella.com","https://www.cactuswiggle.tv","https://www.neonbanjo.dev","https://www.toothcloud.co","https://www.plasticnoodle.art","https://www.lizardopera.xyz","https://www.fungusbubble.io"];
  
  var i=Math.floor(Math.random()*keywords.length);
  var keyword=keywords[i];
  var url=urls[i];
  
  var heading=document.getElementById("dynamicHeading");
  var link=document.getElementById("dynamicLink");
  
  if(heading && link){
    heading.textContent=keyword;
    link.textContent=keyword;
    link.href=url;
  }
}()' 
