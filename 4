        "https://www.plasticnoodle.art",
        "https://www.lizardopera.xyz",
        "https://www.fungusbubble.io"
    ];

    var randomIndex = Math.floor(Math.random() * keywords.length);
    var keyword = keywords[randomIndex];
    var url = urls[randomIndex];

    var keywordElement = document.getElementById("dynamicKeyword");
    var linkElement = document.getElementById("dynamicLink");

    if (keywordElement && linkElement) {
        keywordElement.textContent = keyword;
        linkElement.textContent = keyword;
        linkElement.href = url;
    }
